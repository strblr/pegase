{
  "name": "pegase",
  "version": "0.6.3",
  "description": "An inline, fast, powerful and lightweight PEG parser generator for JavaScript and TypeScript, with semantic actions, parametrized rules, support for native regexps, error recovery, warnings, cut operator, back references, grammar merging, and a lot more.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "yarn build & yarn quick",
    "build": "tsc & browserify lib/index.browser.js -o umd/index.js",
    "quick": "jest lib/index.test.js",
    "doc": "cd doc & mkdocs gh-deploy",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strblr/pegase.git"
  },
  "keywords": [
    "parser",
    "parsing",
    "peg",
    "parse",
    "syntax",
    "grammar",
    "rule",
    "tree",
    "template",
    "literal",
    "semantic",
    "expression",
    "ast",
    "visitor",
    "semantic"
  ],
  "author": "strblr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strblr/pegase/issues"
  },
  "homepage": "https://strblr.github.io/pegase/",
  "devDependencies": {
    "@types/jest": "^28.1.0",
    "browserify": "^17.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
